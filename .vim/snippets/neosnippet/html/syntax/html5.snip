# DOCTYPE {{{
snippet     doct HTML5
abbr        <!DOCTYPE html>
options     head
    <!DOCTYPE html>
    ${0}
# }}}

# <head> {{{
snippet     head
abbr        <head> .. </head>
options     head
    <head>
        ${1:TARGET:code}
    </head>

snippet     head plus
abbr        <head : meta,title,style></head>
options     head
    <head>
        <meta http-equiv="content-type" content="text/html; charset=utf-8">
        <title>${1:#:title}</title>
        <style type="text/css">
            ${2:#:CSS code}
        </style>
        ${3:#:other head tags}
    </head>
    ${0}

# <meta>
snippet meta author
abbr        <meta name="author" content=".."
    <meta name="author" content="${1:stardiviner}">${0}
snippet meta copyright
abbr        <meta name="copyright" content=".."
    <meta name="copyright" content="Â© ${1:stardiviner}">${0}
snippet meta keywords
abbr        <meta name="keywords" content=".."
    <meta name="keywords" content="${1:#:keywords}">${0}
snippet meta generator
abbr        <meta name="generator" content=".."
    <meta name="generator" content="${1:#:generator: Vim}">${0}
snippet meta description
abbr        <meta name="description" content=".."
    <meta name="description" content="${1:#:description}">${0}
snippet meta others
abbr        <meta name="others" content=".."
    <meta name="others" content="${1:#:others}">${0}

snippet     title
abbr        <title> .. </title>
options     head
    <title>${1:TARGET:TITLE}</title>

snippet     link
abbr        <link rel="" href="" media="">
options     head
    <link rel="${1:stylesheet}" href="${2:#:filename}.css" type="text/css" media="${3:screen}" charset="utf-8">${0}

snippet     base
abbr        <base href="">
options     head
    <base href="${1:#:URL}">
# }}}

