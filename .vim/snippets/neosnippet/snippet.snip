snippet     snippet
abbr        snippet abbr alias options ..
alias       snip
options     head
    snippet     ${1:trigger} ${2:descript}
    abbr        ${3:abbr}
    alias       ${4:alias}
    options     ${5:head/word/indent}
        ${6:snippet code: #:comment, TARGET:}

snippet     snippet nested
abbr        ${1:id="${2:nested\}"}
alias       snip_nested,nested_snippet
options     word
    ${${1:N}:${2:placeholder: #:comment, TARGET}\}
